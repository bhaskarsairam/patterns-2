#include <stdio.h>

int main() {
int a[5],i,j,flag;
for(i=0;i<5;i++)
scanf("%d",&a[i]);
printf("The prime numbers in the array \n");
for(i=4;i>=0;i--)
{
    if(a[i]!=1)
    {
    flag=0;
    for(j=2;j<a[i];j++)
    {
        if(a[i]%j==0)
        {
            flag=1;
            break;
        }
    }
    if(flag==0)
    printf("%d\t",a[i]);
    }
}
    return 0;
}
